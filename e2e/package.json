{
  "name": "e2e",
  "version": "1.0.0",
  "description": "E2E testing suite for mus",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:e2e": "./run-tests.sh headless",
