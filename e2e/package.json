    "test:e2e:debug": "./run-tests.sh debug",
    "cleanup": "./cleanup.sh",
    "postinstall": "npx playwright install chromium --with-deps"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.0.0"
  }
}
